<html>
 <head>
  <title>fieldset</title>
 </head>
 <body>
    <form>
	  <h1>set of fields</h1>
	  <fieldset style="width:500px; height:200px;background-color:cyan;">
	  <legend>personal details</legend>
	  <center>
	  <input placeholder="enter name" style="width:300p; height:50px;"></input>
	  <br></br>
	  <input placeholder="enter email" style="width:300p; height:50px;"></input>
	  <br></br>
	  <input placeholder="enter mobile" style="width:300p; height:50px;"></input>
	  <br></br>
	  <input type="submit" value="register"></input>
	  </center>
	  </fieldset>
	</form>
	<br></br>
	  <form>
	  <fieldset style="width:500px; height:200px;background-color:cyan;">
	  <legend>qualification</legend>
	  <center>
	  <input placeholder="college name" style="width:300p; height:50px;"></input>
	  <br></br>
	  <input placeholder="percentage" style="width:300p; height:50px;"></input>
	  <br></br>
	  <input placeholder="degree" style="width:300p; height:50px;"></input>
	  </center>
	  </fieldset>
	</form>
  
 </body>
</html>
