			<LABEL>SELECT JOB LOCATION</LABEL>
			<!--BY DEFAULT TYPE IS TEXT-->
			<INPUT LIS="ABC" REQUIRED PLACEHOLDER="CITY"></INPUT>
			<!--LIST OF OPTION-->
			<DATALIST ID="ABC">
			  <OPTION>HYDERABAD</OPTION>
			  <OPTION>MUMBAI</OPTION>
			  <OPTION>PUNE</OPTION>
			  <OPTION>CHENNAI</OPTION>
			  <OPTION>VIJAWADA</OPTION>
			  <OPTION>BANGOLRE</OPTION>
			  <OPTION>CHANDIGARH</OPTION>
			  <OPTION>NOIDAL</OPTION>
			  <OPTION>THANE</OPTION>
			  <OPTION>GURGAON</OPTION>
			 </DATALIST>
               <BR></BR>
			   <INPUT TYPE="SUBMIT" VALUE="REGISTER"></INPUT>
                
 </body>
</html>
